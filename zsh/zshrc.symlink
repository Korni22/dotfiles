# ZSH config
ZSH=$HOME/.oh-my-zsh
ZSH_THEME="arne"
DISABLE_AUTO_TITLE="true"
COMPLETION_WAITING_DOTS="true"

# Config files
source $HOME/.dotfiles/zsh/*.zsh

# oh my zsh
plugins=(git brew bower coffee node npm xcode)
source $ZSH/oh-my-zsh.sh

# Embed custom per-machine config
[[ -f ~/.localrc ]] && source ~/.localrc
